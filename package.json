{
  "name": "rf-chrome-extension",
  "version": "0.0.1",
  "description": "The Rainforest Chrome Extension for Testers",
  "main": "extension/js/main.js",
  "scripts": {
    "test:mocha": "mocha --require mocha_babel",
    "test:lint": "eslint src",
    "test": "npm run test:mocha && npm run test:lint",
    "testDebug": "mocha debug --require mocha-compiler",
    "build": "mkdir -p dist && babel src/parser.js --out-file dist/index.js",
    "watchTest": "mocha -w --require mocha-compiler",
    "watchTestDebug": "mocha -w debug --require mocha-compiler",
    "test:inspector": "mocha --debug-brk --require mocha-compiler",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainforestapp/tester-chrome-extension.git"
  },
  "keywords": [
    "rainforest",
    "chrome-extension"
  ],
  "author": "Russell Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rainforestapp/tester-chrome-extension/issues"
  },
  "homepage": "https://github.com/rainforestapp/tester-chrome-extension#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "expect": "^1.14.0",
    "jshint": "2.9.1",
    "mocha": "^2.4.5"
  }
}
